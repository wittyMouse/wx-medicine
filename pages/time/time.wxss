.doctor {
    padding: 30rpx;
    background-color: #fff;
}

.doctor .image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.doctor .row {

}

.doctor .row .text {
    
}

.doctor .row .name {
    margin-bottom: 10rpx;
    font-size: 32rpx;
}

.doctor .row .remarks {
    font-size: 26rpx;
    color: var(--colorOne);
}


.time-list {
    background-color: #fff;
}

.time-list .time-time { 
    padding: 40rpx 30rpx;
}

.time-list .time-time .remarks {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    margin-right: 30rpx;
    font-size: 24rpx;
    color: var(--backgroundColorOne);
    text-align: center;
    line-height: 80rpx;
    border: 1px solid var(--backgroundColorOne);
}

.time-list .time-time .remarks.disabled {
    color: #ccc;
    border: 1px solid #ccc;
}

.time-list .time-time .time {
    font-size: 34rpx;
}

.time-list .time-time .label {
    width: 100rpx;
    height: 50rpx; 
    font-size: 30rpx;
    color: #fff;
    text-align: center;
    line-height: 50rpx;
    background-color: var(--backgroundColorOne);
}

.time-list .time-time .label.disabled {
    background-color: #ccc;
}

.time-list .time-time .place {
    width: 50rpx;
    text-align: right;
}

.time-list .time-time .icon-enter {

}
